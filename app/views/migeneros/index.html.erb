<p id="notice"><%= notice %></p>

<h1>Listing Migeneros</h1>

<table>
  <thead>
    <tr>
      <th>Genero</th>
      <th>Idgenero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @migeneros.each do |migenero| %>
      <tr>
        <td><%= migenero.genero %></td>
        <td><%= migenero.idgenero %></td>
        <td><%= link_to 'Show', migenero %></td>
        <td><%= link_to 'Edit', edit_migenero_path(migenero) %></td>
        <td><%= link_to 'Destroy', migenero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Migenero', new_migenero_path %>
